{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:vue/essential"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": ["vue"],
    "rules": {
        "no-console": "off",
        "consistent-return": "off",
        "no-shadow": "off" 
    }
}